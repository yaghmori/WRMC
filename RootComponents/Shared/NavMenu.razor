@inherits BaseComponent


<MudNavMenu Bordered="true" Rounded="true" Color="Color.Primary" Class="pa-2">

    <MudNavLink Href="@AppURL.Dashboard" Match="NavLinkMatch.All" Icon="fad fa-grid-horizontal">@_viewLocalizer[ViewResources.Dashboard]</MudNavLink>
    <MudNavLink Href="@AppURL.Visits" Match="NavLinkMatch.All" Icon="fad fa-calendar-week">@_viewLocalizer[ViewResources.Visits]</MudNavLink>
    <MudNavLink Href="@AppURL.Chat" Icon="fad fa-messages">@_viewLocalizer[ViewResources.Chat]</MudNavLink>
    <MudNavLink Href="@AppURL.Tenants" Icon="fad fa-coins">@_viewLocalizer[ViewResources.Tenants]</MudNavLink>
    <MudText Typo="Typo.subtitle2" Color="Color.Inherit" Class="ml-4 my-3">@_viewLocalizer[ViewResources.IdentityManagement]</MudText>
    <MudNavLink Href="@AppURL.Users" Icon="fad fa-user-group">@_viewLocalizer[ViewResources.Users]</MudNavLink>
    <MudNavLink Href="@AppURL.Roles" Icon="fad fa-user-group">@_viewLocalizer[ViewResources.Roles]</MudNavLink>
    <MudNavLink Href="@AppURL.AppSettings" Icon="fad fa-gears">@_viewLocalizer[ViewResources.AppSettings]</MudNavLink>
    <MudNavLink Href="@AppURL.Logout" Icon="fad fa-right-from-bracket">@_viewLocalizer[ViewResources.Logout]</MudNavLink>

</MudNavMenu>



